{
    "name": "nemo64/canonical-url",
    "description": "Makes your typo3 aware of the url it is used on",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Marco Pfeiffer",
            "email": "git@marco.zone"
        }
    ],
    "require": {
        "php": "~7.0",
        "typo3/cms-core": "~8.7.7 || ^9.5",
        "typo3/cms-frontend": "~8.7.7 || ^9.5"
    },
    "replace": {
        "canonical_url": "self.version",
        "typo3-ter/canonical-url": "self.version"
    },
    "require-dev": {
        "typo3/cms-composer-installers": ">=1.4"
    },
    "autoload": {
        "psr-4": {
            "Nemo64\\CanonicalUrl\\": "Classes"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "typo3/cms": {
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
